<table class="c-top-nav c-top-nav--shallow">
  <tbody>
    <tr>
      
      <td class="c-top-nav__nav-section c-top-nav__nav-section--half">
        
        <%= link_to image_tag("nu_color_logo.svg", class: "c-top-nav__nav-section__img" ), root_path %>

      </td>
      
      <td class="c-top-nav__nav-section c-top-nav__nav-section--half">

        <ul class="o-inline-list">

          <li class="o-inline-list__li">
            <%= link_to "About", about_general_path, class: "c-top-nav__nav-section__text-link" %>
          </li>

          <li class="o-inline-list__li">
            <%= link_to "Feedback", new_feedback_path, class: "c-top-nav__nav-section__text-link" %>
          </li>

          <% if session[:user_id] %>
            <li class="o-inline-list__li">
              <%= link_to("Logout", logout_path, method: :delete) %>  
            </li>
          <% end %>
        </ul>

      </td>

    </tr>
