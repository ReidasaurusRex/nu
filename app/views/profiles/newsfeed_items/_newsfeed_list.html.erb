<div class="c-newsfeed-list">
<% newsfeed_items.each do |feed_item| %>
  
  <div class="o-bubble o-bubble--no-pad c-newsfeed-list__item">
    
    <div class="c-newsfeed-list__item--padding">
    <% if feed_item.source_type == "profile" %>
      <%= render partial: "profiles/newsfeed_items/native_item", locals: {profile: @profile, item: feed_item} %> 
    <% else %>
      <%= render partial: "profiles/newsfeed_items/feed_item", locals: {profile: @profile, item: feed_item} %>
    <% end %>

    <ul class="o-inline-list c-social-overview">
      <li class="o-inline-list__li c-social-overview__likes c-social-overview__li">
        <%= like_button(feed_item, profile) %> <span class="u-text--edward"><%= like_number(feed_item) %></span>
      </li>
      <li class="o-inline-list__li c-social-overview__comments c-social-overview__li">
        <%= comment_button(feed_item, profile) %> <span class="u-text--edward"><%= comment_number(feed_item) %></span>
      </li>
    </ul>
  </div> <!-- c-newsfeed-list-item--padding -->

    <ul class="c-comments-list">
      <%= render partial: "profiles/comments/section_overview", locals: {newsfeed_item: feed_item, profile: profile} %>
    </ul>

  </div>

<% end %>
</div> 